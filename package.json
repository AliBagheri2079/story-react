    "postcss-preset-env": "^8.4.1",
    "postcss-scss": "^4.0.6",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^2.8.8",
    "stylelint": "^15.6.2",
    "stylelint-config-standard": "^33.0.0",
