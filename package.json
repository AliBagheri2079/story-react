{
  "name": "story-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint:scripts": "eslint src --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:scripts:fix": "pnpm run lint:scripts --fix",
    "lint:styles": "stylelint ./src/**/*.css",
