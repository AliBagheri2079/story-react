    "format:scripts": "prettier src --check",
    "format:scripts:fix": "pnpm run format:scripts --write",
    "format:styles": "pnpm run lint:styles --fix",
    "format": "pnpm run format:scripts:fix & pnpm run format:styles",
    "prepare": "husky install"
  },
  "dependencies": {
